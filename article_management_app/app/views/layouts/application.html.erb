<!DOCTYPE html>
<html>
<head>
  <title>MongoDBExampleApp</title>
  <!--
  <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css' %>
  <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js' %>
  -->
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <!--
  <div class="container">
  -->
    <!-- Will render navigation using partial _navigation.html.erb -->

    <%= render 'layouts/navigation' %>

    <!-- Will call the partial _messages.html.erb to render any message (article created, etc) -->
    <%= render 'layouts/messages' %>
    <!-- Any content inside views/**/*.html.erb will render under this div -->
    <div class="container">
      <%= yield %>
    </div>
    <%= render 'layouts/footer' %>
    <!--
  </div>
    -->
  
  <%= debug(params) if Rails.env.development? %>
</body>
</html>